{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["react"],
    "rules": {
        "react/no-unescaped-entities": "off",
        "react/prop-types": "off",
        "no-const-assign": "off",
        "no-undef": "off"
    }
}
