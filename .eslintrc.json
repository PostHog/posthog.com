{
    "env": {
        "browser": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": ["react"],
    "rules": {
        "react/no-unescaped-entities": "off",
        "react/prop-types": "off",
        "no-const-assign": "off",
        "react/jsx-key": "off"
    }
}
